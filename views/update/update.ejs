<html>
  <head>
    <Title>
      Database | Update
    </Title>
  </head>

  <body>
    <form method="POST" action="/update/<%= update.id %>">
      <label for="username">
        username
      </label>
      <br>
      <input type="text" name="username" value="<%= update.username %>">
      <br>

      <label for="password">
        password
      </label>
      <br>
      <input type="password" name="password" value="<%= update.password %>">
      <br>

      <label for="name">
        Name
      </label>
      <br>
      <input type="text" name="name" value="<%= update.user_game_biodatas.name %>">
      <br>

      <label for="age">
        Age
      </label>
      <br>
      <input type="text" name="age" value="<%= update.user_game_biodatas.age %>">
      <br>
      <br>
      <button type="submit">
        Submit
      </button>
    </form>
  </body>
</html>